  "scripts": {
    "reset": "npx nx reset",
    "build": "touch ./packages/getmoonbeam-cdk/graphql/schema.graphql && npx nx run-many -t build",
    "clean": "npx nx run-many -t clean && rm -rf ./node_modules",
    "postinstall": "husky install",
    "release": "yarn run clean && yarn install && yarn run build"
  },
  "devDependencies": {
    "husky": "^8.0.3",
    "nx": "17.3.1",
    "nx-cloud": "^16.5.2",
    "patch-package": "^8.0.0",
    "typescript": "^4.9.4",
    "axios": "^1.4.0"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ],
    "nohoist": [
    ]
  },
  "private": true
}
